<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="Gridbox">
    <attr name="inner_margin" format="dimension" />
    <attr name="gravity" format="integer">
        <flag name="center_horizontal" value="0x01" />
        <flag name="left" value="0x03" />
        <flag name="right" value="0x05" />
        <flag name="fill_horizontal" value="0x07" />
        <flag name="center_vertical" value="0x10" />
        <flag name="top" value="0x30" />
        <flag name="bottom" value="0x50" />
        <flag name="fill_vertical" value="0x70" />
        <flag name="center" value="0x11" />
        <flag name="fill" value="0x77" />
    </attr>
    <attr name="force_uniform_width" format="boolean" />
    <attr name="force_uniform_height" format="boolean" />
 </declare-styleable>
  <declare-styleable name="Gridbox_Layout">
    <!--
    <attr name="layout_margin" format="dimension" />
    <attr name="layout_marginLeft" format="dimension" />
    <attr name="layout_marginRight" format="dimension" />
    <attr name="layout_marginBottom" format="dimension" />
    <attr name="layout_marginTop" format="dimension" />
    -->
    <attr name="layout_gridx" format="integer" />
    <attr name="layout_gridy" format="integer" />
    <attr name="layout_colSpan" format="integer" />
    <attr name="layout_rowSpan" format="integer" />
    <attr name="layout_weightx" format="float" />
    <attr name="layout_weighty" format="float" />
    <attr name="layout_gravity">
        <flag name="center_horizontal" value="0x01" />
        <flag name="left" value="0x03" />
        <flag name="right" value="0x05" />
        <flag name="fill_horizontal" value="0x07" />
        <flag name="center_vertical" value="0x10" />
        <flag name="top" value="0x30" />
        <flag name="bottom" value="0x50" />
        <flag name="fill_vertical" value="0x70" />
        <flag name="center" value="0x11" />
        <flag name="fill" value="0x77" />
    </attr>
 </declare-styleable>
</resources>
